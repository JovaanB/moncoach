{
  "expo": {
    "name": "Mon coach",
    "slug": "mon-coach",
    "version": "1.0.0",
    "scheme": "moncoach",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-secure-store",
      "expo-router"
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "config": {
        "useNonExcemptEncryption": false
      },
      "bundleIdentifier": "com.moncoach.expoappleauth"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "7a99fba5-ce6e-485d-b848-b72779f11210"
      }
    }
  }
}
